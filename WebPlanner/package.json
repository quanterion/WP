{
  "name": "web-planner",
  "version": "1.0.0-beta.2",
  "license": "Commercial",
  "scripts": {
    "ngx": "node --max_old_space_size=3072 ./node_modules/@angular/cli/bin/ng",
    "xliffmerge": "xliffmerge",
    "buildprod": "yarn run ngx build Web-Planner --prod --aot --progress=false --build-optimizer=false",
    "buildprodru": "yarn run ngx build Web-Planner --prod --output-path=dist-ru --i18n-locale ru --i18n-format xlf --i18n-file src/locale/messages.ru.xlf --progress=false --source-map",
    "buildprodfi": "yarn run ngx build Web-Planner --prod --configuration=fi --progress=false --source-map",
    "buildprodembedru": "yarn run ngx build Web-Embed --prod --output-path=dist-ru-embed --i18n-locale ru --i18n-format xlf --i18n-file src/locale/messages.ru.xlf --progress=false --source-map",
    "buildru": "yarn run buildprodru && yarn run buildprodembedru",
    "start": "yarn && yarn run ngx serve",
    "start-wigwam": "yarn && yarn run ngx serve --proxy-config configs/wigwam.proxy.js",
    "start-angstrem": "yarn && yarn run ngx serve --proxy-config configs/angstrem.proxy.js",
    "start-docker": "yarn && yarn run ngx serve --proxy-config configs/docker.proxy.js",
    "start-ru": "yarn run ngx serve --configuration=ru",
    "start-wigwam-ru": "yarn run ngx serve --configuration=wigwam.ru",
    "start-wigwam-aot": "yarn run ngx serve --proxy-config configs/wigwam.proxy.js",
    "startembed": "yarn run ngx serve Web-Embed",
    "start-angstrem-embed": "yarn && yarn run ngx serve Web-Embed --proxy-config configs/angstrem-embed.proxy.js --aot",
    "start-viewer": "yarn && yarn run ngx serve Web-Viewer",
    "build-viewer": "yarn run ngx build Web-Viewer --prod --output-path=dist-viewer --aot --progress=false --source-map",
    "online-viewer": "yarn && yarn run ngx serve --proxy-config configs/viewer.proxy.js Web-Viewer",
    "lint": "ng lint",
    "test": "ng test Web-Planner",
    "testci": "ng test Web-Planner --watch=false --browsers=Chrome_CI --progress=false --prod",
    "e2e": "ng e2e --proxy-config configs/docker.proxy.js --environment=e2e",
    "e2e-stage": "ng e2e Web-Planner-stage",
    "i18n": "ng xi18n --outputPath locale --outFile messages.xlf && xliffmerge --profile xliffmerge.json",
    "xm": "xliffmerge --profile xliffmerge.json",
    "build-server": "webpack --config webpack.server.config.js --env=production",
    "run-server": "webpack --config webpack.server.config.js --progress --colors && node dist-server/server --test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.3",
    "@angular/cdk": "8.1.3",
    "@angular/common": "8.2.3",
    "@angular/compiler": "8.2.3",
    "@angular/core": "8.2.3",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "8.2.3",
    "@angular/material": "8.1.3",
    "@angular/platform-browser": "8.2.3",
    "@angular/platform-browser-dynamic": "8.2.3",
    "@angular/platform-server": "8.2.3",
    "@angular/router": "8.2.3",
    "@aspnet/signalr": "^1.1.4",
    "@ngx-share/core": "7.1.2",
    "@sentry/browser": "5.6.2",
    "@types/pako": "1.0.0",
    "angulartics2": "8.0.0",
    "ckeditor": "git://github.com/quanterion/ckeditor5-build-webplanner#1a1eef2e32ab6e8358aa21e01dfc4322545fc760",
    "core-js": "2.5.4",
    "hammerjs": "2.0.8",
    "injection-js": "^2.2.2",
    "jsdom": "^15.2.0",
    "long": "3.2.0",
    "monaco-editor": "0.18.1",
    "pako": "1.0.10",
    "protobufjs": "6.8.8",
    "rxjs": "6.5.2",
    "xhr2": "^0.2.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.1",
    "@angular/cli": "8.3.1",
    "@angular/compiler-cli": "8.2.3",
    "@angular/language-service": "8.2.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-i18nsupport": "0.16.1",
    "protractor": "5.4.1",
    "ts-loader": "^5.2.0",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.5.3",
    "webpack-cli": "^3.1.0"
  }
}
